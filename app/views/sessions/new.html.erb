<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>

<%= form_for :sessions, url: login_path do |f| %>
    <%= f.label :email, "Email" %>
    <%= f.text_field :email %>
    <br>
    <%= f.label :password, "Password" %>
    <%= f.password_field :password %>
    <br>
    <%= f.submit "Login" %>
<% end %>
<%= link_to  "New? Signup", sign_up_path %>