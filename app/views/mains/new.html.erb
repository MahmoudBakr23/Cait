<h1>Mains#new</h1>
<p>Find me in app/views/mains/new.html.erb</p>

<%= form_for @main do |f| %>
    <%= f.label :head, "Headline" %>
    <%= f.text_field :head %>
    <br>
    <%= f.label :body, "Body" %>
    <%= f.text_area :body %>
    <br>
    <%= f.label :image, "Upload an image" %>
    <%= f.file_field :image %>
    <br>
    <%= f.submit "Create Main" %>
<% end %>